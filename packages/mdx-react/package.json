{
  "name": "@temelj/mdx-react",
  "description": "React utilities for MDX processing.",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tinrab/temelj"
  },
  "version": "0.9.0",
  "type": "module",
  "main": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "clean": "shx rm -rf dist"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@temelj/mdx": "workspace:*",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "25.0.10",
    "@types/react": "^19.2.9",
    "tsdown": "^0.20.1",
    "shx": "^0.4.0",
    "typescript": "^5.9.3"
  }
}

{
  "name": "@temelj/mdx-react",
  "description": "React utilities for MDX processing.",
  "license": "MIT",
  "version": "0.8.0",
  "type": "module",
  "main": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "shx rm -rf dist"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@temelj/mdx": "workspace:*",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "24.10.0",
    "@types/react": "^19.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}

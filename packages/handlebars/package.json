{
  "name": "@temelj/handlebars",
  "description": "Handlebars helpers and utilities.",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tinrab/temelj"
  },
  "version": "0.9.0",
  "type": "module",
  "main": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "shx rm -rf dist"
  },
  "dependencies": {
    "@temelj/array": "workspace:*",
    "@temelj/string": "workspace:*",
    "@temelj/value": "workspace:*",
    "handlebars": "^4.7.8",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "25.0.10",
    "tsdown": "^0.20.1",
    "shx": "^0.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}

    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "tsup": "^8.5.1",
    "shx": "^0.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}

{
  "name": "@temelj/handlebars",
  "description": "Handlebars helpers and utilities.",
  "license": "MIT",
  "version": "0.8.1",
  "type": "module",
  "main": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.js"
